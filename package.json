{
  "name": "react-native-ble-qnsdk",
  "title": "React Native Ble Qnsdk",
  "version": "1.2.17",
  "description": "A React Native module for handling Bluetooth interactions with Yolanda scales",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "react-native": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coachcare/react-native-ble-qnsdk.git",
    "baseUrl": "https://github.com/coachcare/react-native-ble-qnsdk"
  },
  "files": [
    "dist",
    "android",
    "ios",
    "react-native-ble-qnsdk.podspec",
    "react-native.config.js",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "codegenConfig": {
    "name": "BleQnsdkSpec",
    "type": "modules",
    "jsSrcsDir": "src",
    "android": {
      "javaPackageName": "com.coachcare"
    }
  },
  "keywords": [
    "react-native",
    "bluetooth",
    "yolanda scales",
    "weight measurement"
  ],
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/integration-tests",
      "<rootDir>/node_modules"
    ]
  },
  "author": {
    "name": "Jeff",
    "email": "jeffdrakos@gmail.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "devDependencies": {
    "react": "18.3.1",
    "react-native": "0.76.3",
    "typescript": "^5.1.6",
    "@react-native-community/cli": "latest"
  }
}
